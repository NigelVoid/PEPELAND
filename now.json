{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "server.py"
    }
  ],
  "functions": {
    "api": {
      "runtime": "python3.8",
      "memory": 1024
    },
    "environment": {
      "FLASK_ENV": "production"
    }
  }
}